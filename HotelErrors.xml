<?xml version="1.0" encoding="utf-8"?>

<Hotels xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="Hotels.xsd">

  <!-- 1) Missing required attribute 'id' on this Hotel (schema requires id) -->
  <Hotel>
    <Name>Westin</Name>
    <Phone>480-968-8885</Phone>
    <Address NearestAirport="Sky Harbor">
      <Number>11</Number>
      <Street>E.7th St</Street>
      <City>Tempe</City>
      <State>AZ</State>
      <Zip>85281</Zip>
    </Address>
    <Rating>4.2</Rating>
  </Hotel>

  <!-- 2) Address missing the required attribute NearestAirport -->
  <Hotel id="E0002">
    <Name>Hilton Garden Inn Phoenix</Name>
    <Phone>602-272-6888</Phone>
    <Address>
      <Number>2045</Number>
      <Street>East Indian School Rd</Street>
      <City>Phoenix</City>
      <State>AZ</State>
      <Zip>85016</Zip>
    </Address>
  </Hotel>

  <!-- 3) Duplicate Name element (schema expects only one Name in sequence) -->
  <Hotel id="E0003">
    <Name>Embassy Suites Phoenix</Name>
    <Name>Embassy Suites Duplicate</Name>
    <Phone>602-553-9999</Phone>
    <Address NearestAirport="Sky Harbor">
      <Number>4415</Number>
      <Street>East Washington St</Street>
      <City>Phoenix</City>
      <State>AZ</State>
      <Zip>85034</Zip>
    </Address>
  </Hotel>  <!-- 4) Wrong element order: Rating comes before Address (schema sequence requires Address then Rating) -->
  <Hotel id="E0004">
    <Name>Omni Scottsdale Resort</Name>
    <Phone>480-948-2100</Phone>
    <Rating>4.3</Rating> <!-- out of order -->
    <Address NearestAirport="Sky Harbor">
      <Number>7000</Number>
      <Street>East Camelback Rd</Street>
      <City>Scottsdale</City>
      <State>AZ</State>
      <Zip>85251</Zip>
    </Address>
  </Hotel>

  <!-- 5) Unexpected element 'Extra' (not declared in schema under Hotel) -->
  <Hotel id="E0005">
    <Name>Arizona Grand Resort</Name>
    <Phone>480-789-5300</Phone>
    <Address NearestAirport="Sky Harbor">
      <Number>8000</Number>
      <Street>South Arizona Grand Pkwy</Street>
      <City>Phoenix</City>
      <State>AZ</State>
      <Zip>85044</Zip>
    </Address>
    <Extra>this element is not in the schema</Extra>
  </Hotel>

</Hotels>