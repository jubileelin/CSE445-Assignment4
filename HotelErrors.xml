<?xml version="1.0" encoding="utf-8"?>

<!-- 1) Root element intentionally incorrect (should be <Hotels>) -->
<HotelRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="Hotels.xsd">

  <!-- 2) Missing required attribute 'id' here (schema requires id on Hotel) -->
  <Hotel>
    <Name>Westin</Name>
    <Phone>480-968-8885</Phone>
    <Phone>800-937-8461</Phone>
    <!-- 3) Missing NearestAirport attribute on Address (schema requires it) -->
    <Address>
      <Number>11</Number>
      <Street>E.7th St</Street>
      <City>Tempe</City>
      <State>AZ</State>
      <Zip>85281</Zip>
    </Address>
    <Rating>4.2</Rating>
  </Hotel>

  <!-- 4) Duplicate Name element (schema sequence allows only one Name) -->
  <Hotel id="E0002">
    <Name>Hilton Garden Inn Phoenix</Name>
    <Name>Hilton Duplicate Name</Name>
    <Phone>602-272-6888</Phone>
    <Address NearestAirport="Sky Harbor">
      <Number>2045</Number>
      <Street>East Indian School Rd</Street>
      <City>Phoenix</City>
      <State>AZ</State>
      <Zip>85016</Zip>
    </Address>
  </Hotel>

  <!-- 5) Wrong element order: Rating appears before Address (schema enforces sequence). -->
  <Hotel id="E0003">
    <Name>Embassy Suites Phoenix</Name>
    <Phone>602-553-9999</Phone>
    <Rating>4.0</Rating>
    <Address NearestAirport="Sky Harbor">
      <Number>4415</Number>
      <Street>East Washington St</Street>
      <City>Phoenix</City>
      <State>AZ</State>
      <Zip>85034</Zip>
    </Address>
  </Hotel>  
  <Hotel id="E0004">
    <Name>Omni Scottsdale Resort</Name>
    <Phone>480-948-2100</Phone>
    <Address NearestAirport="Sky Harbor">
      <Number>7000</Number>
      <Street>East Camelback Rd</Street>
      <City>Scottsdale</City>
      <State>AZ</State>
      <Zip>85251</Zip>
    </Address>
  </Hotel>

</HotelRoot>